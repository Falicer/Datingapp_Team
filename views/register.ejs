<h2>Register an account</h2>
<% if (messages.error) { %> <%= locals.messages.error %> <% } %>
<form action="/register" method="POST">
  <div>
    <label for="name">Name</label>
    <input type="text" id="name" name="name" required value="Jesse" />
  </div>
  <div>
    <label for="email">Email</label>
    <input type="email" id="email" name="email" required value="anna@live.nl" />
  </div>
  <div>
    <label for="password">Password</label>
    <input type="password" id="password" name="password" required value="123" />
  </div>

  <div>
    <label for="password2">Repeat Password</label>
    <input
      type="password"
      id="password2"
      name="password2"
      required
      value="123"
    />
  </div>
  <button type="submit">Register</button>
</form>
<a href="/login">Login</a>
